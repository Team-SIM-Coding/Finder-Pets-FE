{
  "extends": "../../tsconfig.service.json",
  "compilerOptions": {
    "target": "ES6",
    "moduleResolution": "bundler",
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/api/*": ["./src/app/api/*"],
      "@/components/*": ["./src/components/*"],
      "@/contexts/*": ["./src/contexts/*"],
      "@/hooks/*": ["./src/hooks/*"],
      "@/models/*": ["./src/models/*"],
      "@/react-query/*": ["./src/react-query/*"],
      "@/recoil/*": ["./src/recoil/*"],
      "@/shared/c/*": ["./src/shared/components/*"],
      "@/shared/*": ["./src/shared/*"],
      "@/mocks/*": ["./src/shared/mocks/*"],
      "@/styles/*": ["./src/shared/styles/*"],
      "@/utils/*": ["./src/utils/*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ],
    "skipLibCheck": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
